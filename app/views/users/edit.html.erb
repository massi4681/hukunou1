<h1>登録情報編集</h1>
<%= form_for(@user,url: users_update_path) do |f| %>
  <div class="field">
    <%= f.label :'会社名' %><br />
    <%= f.text_field :name, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :'郵便番号(ハイフンなし)' %><br />
    <%= f.text_field :postal_code, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :住所 %><br />
    <%= f.text_field :adress, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :'電話番号(ハイフンなし)' %><br />
    <%= f.text_field :telephone_number, autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :メールアドレス %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

    <%= f.submit "編集内容を保存する" %>

    <% end %>

